<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
  <!--
    Replace the domain below with the host you need to allow cleartext (HTTP) for.
    Prefer HTTPS for production and limit cleartext to a single domain for testing.
  -->
  <domain-config cleartextTrafficPermitted="true">
    <domain includeSubdomains="false">91.98.231.10</domain>
  </domain-config>
  <base-config cleartextTrafficPermitted="false" />
</network-security-config>
