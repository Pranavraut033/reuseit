{
  "extends": [
    "expo/tsconfig.base"
  ],
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "~/*": [
        "*"
      ],
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ]
    }
  },
  "include": [
    "codegen.ts",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}